{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rmodeladf"
		},
		"databricks_properties_typeProperties_existingClusterId": {
			"type": "string"
		},
		"kcmunninadlsgen2mpa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://databricks@kcmunninadlsgen2mpa.dfs.core.windows.net"
		},
		"kcmunninadlsgen2mpa_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"kcmunninadlsgen2mpa_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "c74ca4a7-02f5-46ec-88c8-2136222c3d74"
		},
		"keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kmdevopskv.vault.azure.net/"
		},
		"adlsgen2-trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/bc54e366-6543-43f7-b404-cc14e4f525ab/resourceGroups/rg_persisteditems/providers/Microsoft.Storage/storageAccounts/kcmunninadlsgen2mpa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('keyvault_properties_typeProperties_baseUrl')]"
					},
					"databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('databricks_properties_typeProperties_existingClusterId')]"
					},
					"kcmunninadlsgen2mpa_properties_typeProperties_url": {
						"value": "[parameters('kcmunninadlsgen2mpa_properties_typeProperties_url')]"
					},
					"kcmunninadlsgen2mpa_properties_typeProperties_tenant": {
						"value": "[parameters('kcmunninadlsgen2mpa_properties_typeProperties_tenant')]"
					},
					"kcmunninadlsgen2mpa_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('kcmunninadlsgen2mpa_properties_typeProperties_servicePrincipalId')]"
					},
					"adlsgen2-trigger_properties_typeProperties_scope": {
						"value": "[parameters('adlsgen2-trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}